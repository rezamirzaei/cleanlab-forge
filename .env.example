# Copy this file to `.env` and fill in values.

# (Recommended) App-prefixed keys (the app propagates these to provider env vars)
# CLEANLAB_DEMO_OPENAI_API_KEY=your_openai_key_here
# CLEANLAB_DEMO_ANTHROPIC_API_KEY=your_anthropic_key_here

# Provider-native keys also work (loaded from `.env` into process env vars)
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Optional: override paths (relative paths are resolved from the project root)
# CLEANLAB_DEMO_DATA_DIR=data
# CLEANLAB_DEMO_ARTIFACTS_DIR=artifacts
# CLEANLAB_DEMO_LOG_LEVEL=INFO

